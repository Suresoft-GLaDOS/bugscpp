  "common": {
    "build": {
      "command": {
        "type": "docker",
        "lines": [
          "./config enable-unit-test",
          "make clean",
          "make -j@DPP_PARALLEL_BUILD@ depend",
          "@DPP_GEN_COMPILATION_DB_TOOL@ make -j@DPP_PARALLEL_BUILD@"
        ]
      }
    },
    "build-coverage": {
      "command": {
        "type": "docker",
        "lines": [
          "./config CFLAGS=\"--coverage -g -O0\" LDFLAGS=\"-lgcov\" enable-unit-test",
          "make clean",
          "make -j@DPP_PARALLEL_BUILD@ depend",
          "@DPP_GEN_COMPILATION_DB_TOOL@ make -j@DPP_PARALLEL_BUILD@"
        ]
      }
    },
    "test-type": "automake",
    "test": {
      "command": {
        "type": "docker",
        "lines": [
          "bash -c '[ -f AUTOMAKE_TEST_CASE.output ] || make list-tests | grep test_ > AUTOMAKE_TEST_CASE.output'",
          "bash -c 'make -j@DPP_PARALLEL_BUILD@ test TESTS=$(head -$(cat DPP_TEST_INDEX) AUTOMAKE_TEST_CASE.output | tail -1)'"
        ]
      }
    },
    "test-coverage": {
      "command": {
        "type": "docker",
        "lines": [
          "bash -c '[ -f AUTOMAKE_TEST_CASE.output ] || make list-tests | grep test_ > AUTOMAKE_TEST_CASE.output'",
          "bash -c 'make LDFLAGS=\"-lgcov\" -j@DPP_PARALLEL_BUILD@ test TESTS=$(head -$(cat DPP_TEST_INDEX) AUTOMAKE_TEST_CASE.output | tail -1)'"
        ]
      }
    },
    "gcov": {
      "exclude": [],
      "command": {
        "type": "docker",
        "lines": [
          "mkdir gcov",
          "bash -c 'find /home/workspace/apps -type f -name \"*.o\" -exec gcov --preserve-paths {} \\;'",
          "bash -c 'find /home/workspace/apps -type f -name \"*.gcda\" -delete'",
          "bash -c 'find /home/workspace/crypto -type f -name \"*.o\" -exec gcov --preserve-paths {} \\;'",
          "bash -c 'find /home/workspace/crypto -type f -name \"*.gcda\" -delete'",
          "bash -c 'find /home/workspace/providers -type f -name \"*.o\" -exec gcov --preserve-paths {} \\;'",
          "bash -c 'find /home/workspace/providers -type f -name \"*.gcda\" -delete'",
          "bash -c 'find /home/workspace/ssl -type f -name \"*.o\" -exec gcov --preserve-paths {} \\;'",
          "bash -c 'find /home/workspace/ssl -type f -name \"*.gcda\" -delete'",
          "bash -c 'mv *.gcov /home/workspace/gcov'"
        ]
      }
    }
  },
